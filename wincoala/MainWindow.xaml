<Window x:Class="wincoala.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel Orientation="Horizontal" Height="35" VerticalAlignment="Top">
            <Button x:Name="ButtonLintOnline" Content="Lint Online" MinWidth="100" Click="ButtonLintOnline_Click"></Button>
            <Button x:Name="ButtonBearList"   Content="Language"    MinWidth="100" Click="ButtonBearList_Click"  Margin="10,0,0,0" ></Button>
        </StackPanel>
        <Frame x:Name="MainFrame" Margin="0,35,0,0" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
